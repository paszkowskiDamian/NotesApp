.note-content {
    word-wrap: break-word;
}

.note-images{
    background-color: #f6f6f6;
    margin-right: 5px;
    margin-top: 5px;
    box-sizing: border-box;
    height: 20vh;
}

.note-menu > small {
    margin-right: 10px;
    cursor: default;
}

.note-menu > small:hover{
    font-weight: bold;
}

.note-edit{
    overflow: auto;
}

.note{
    margin-top: 1em;
}

.note h3 {
    margin-bottom: 0;
}